<h1>Tweets</h1>

<div class="row-fluid">
  <div class="span4 well">
    <%= form_for (@tweet) do |f| %>
     <p><%= f.text_field(:content)%></p>
     <p><%= f.submit("Tweet it")%></p>
    <% end %>
  </div><!--/.well -->
  <div class="span6">
    <div class="well">
      <h1>Feed</h1>
      <% @user.tweets.each do |post| %>
      <p> <%= post.content %> </p>
      <% end %>
    </div>
  </div>
</div><!--/row-->
